{
  "name": "paperhive-frontend",
  "version": "0.0.0",
  "description": "PaperHive frontend",
  "scripts": {
    "build": "npm run build:css",
    "build:debug": "npm run build:css:debug",
    "build:css": "lessc --source-map --source-map-less-inline --clean-css=\"--skip-restructuring\" src/less/index.less build/index.css",
    "build:css:debug": "lessc --source-map --source-map-less-inline src/less/index.less build/index.css",
    "build:js": "",
    "build:templates": "gulp templates",
    "clean": "rm -rf build/* tmp/*",
    "lint": "npm run lint:html && npm run lint:jshint && npm run lint:jscs && npm run lint:less",
    "lint:html": "htmlhint src/index.html src/templates",
    "lint:jscs": "jscs --reporter ./node_modules/jscs-stylish *.js src test",
    "lint:jshint": "jshint *.js src test",
    "lint:less": "lessc --lint src/less/index.less",
    "test": "npm run lint && npm run test:karma",
    "test:karma": "karma start --single-run"
  },
  "author": "Nico Schl√∂mer <nico.schloemer@gmail.com>",
  "license": "GPL-3.0",
  "private": true,
  "devDependencies": {
    "bower": "^1.4.1",
    "browserify": "^11.0.0",
    "browserify-shim": "^3.8.10",
    "clean-css": "^3.3.6",
    "connect-history-api-fallback": "^1.1.0",
    "gulp": "^3.8.11",
    "gulp-angular-templatecache": "^1.6.0",
    "gulp-connect": "^2.2.0",
    "gulp-htmlmin": "^1.1.1",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.1.1",
    "gulp-protractor": "1.0.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-streamify": "0.0.5",
    "gulp-template": "^3.0.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "htmlhint": "^0.9.7",
    "jasmine": "^2.3.1",
    "jasmine-core": "^2.3.4",
    "jscs": "^1.13.1",
    "jscs-stylish": "^0.3.1",
    "jshint": "^2.8.0",
    "karma": "^0.13.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.0",
    "less": "^2.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "~3.8.0",
    "merge-stream": "^0.1.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.1.2"
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "angular-animate": "./bower_components/angular-animate/angular-animate.js",
    "angular-bootstrap-tpls": "./bower_components/angular-bootstrap/ui-bootstrap-tpls.js",
    "angular-leaflet-directive": "./bower_components/angular-leaflet-directive/dist/angular-leaflet-directive.js",
    "angular-moment": "./bower_components/angular-moment/angular-moment.js",
    "angular-route": "./bower_components/angular-route/angular-route.js",
    "angular-route-segment": "./bower_components/angular-route-segment/build/angular-route-segment.js",
    "angular-sanitize": "./bower_components/angular-sanitize/angular-sanitize.js",
    "detect-element-resize": "./bower_components/javascript-detect-element-resize/jquery.resize.js",
    "highlightjs": "./bower_components/highlightjs/highlight.pack.js",
    "leaflet": "./bower_components/leaflet/dist/leaflet.js",
    "pdfjs": "./bower_components/pdfjs-dist/build/pdf.js",
    "pdfjs-compatibility": "./bower_components/pdfjs-dist/web/compatibility.js",
    "mentio": "./bower_components/ment.io/dist/mentio.js",
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "kramed": "./bower_components/kramed/lib/kramed.js",
    "moment": "./bower_components/moment/moment.js",
    "rangy": "./bower_components/rangy/rangy-core.js",
    "rangy-classapplier": "./bower_components/rangy/rangy-classapplier.js",
    "rangy-highlighter": "./bower_components/rangy/rangy-highlighter.js",
    "rangy-serializer": "./bower_components/rangy/rangy-serializer.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular",
      "depends": [
        "jquery"
      ]
    },
    "angular-animate": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-bootstrap-tpls": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-leaflet-directive": {
      "exports": null,
      "depends": [
        "angular",
        "leaflet:L"
      ]
    },
    "angular-moment": {
      "exports": "angular-moment",
      "depends": [
        "angular",
        "moment"
      ]
    },
    "angular-route": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-route-segment": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-sanitize": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "detect-element-resize": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "jquery": {
      "exports": "$"
    },
    "leaflet": {
      "exports": "L"
    },
    "moment": {
      "exports": "moment"
    },
    "pdfjs": {
      "exports": "PDFJS"
    },
    "pdfjs-compatibility": {
      "depends": [
        "pdfjs:PDFJS"
      ]
    },
    "highlightjs": {
      "exports": "hljs"
    }
  }
}
